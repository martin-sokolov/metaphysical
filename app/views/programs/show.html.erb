<br/>
<h3><%= @program.title %></h3>
<% @videos.each do |v| %>
<div style="position: relative;">
  <video class="video-js vjs-default-skin" controls preload="none" width="700" height="350"  poster="/uploads/<%= v.thumb %>" data-setup="{}">
  <source src="/uploads/<%= v.url %>" type='video/mp4' />
</video>
  <div style="position: absolute;left: 0px;top:0px;background-color: #000000;height:40px;min-width:700px;text-align: center;opacity: 0.7;"><h4 style="color: #ffffff;"><%= v.title %></h4></div>
</div>
<% end %>